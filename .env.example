# Archivo de ejemplo para variables de entorno
# Copia este archivo como .env.staging o .env.production y ajusta los valores

# Docker Configuration
DOCKER_REGISTRY=docker.io
DOCKER_REPO=gymetra
IMAGE_TAG=latest

# Database Configuration - CAMBIAR POR VALORES REALES
POSTGRES_PASSWORD=CAMBIAR_POR_PASSWORD_SEGURO
POSTGRES_DB=gymdb

# Application Configuration
SPRING_PROFILES_ACTIVE=staging
LOG_LEVEL=INFO

# Email Configuration - CAMBIAR POR VALORES REALES
MAIL_HOST=smtp.gmail.com
MAIL_PORT=587
MAIL_USERNAME=CAMBIAR_POR_EMAIL_REAL
MAIL_PASSWORD=CAMBIAR_POR_PASSWORD_EMAIL

# JWT Configuration - CAMBIAR POR VALOR SEGURO
JWT_SECRET=CAMBIAR_POR_JWT_SECRET_MUY_SEGURO_Y_LARGO
JWT_EXPIRATION=36000000

# CORS Configuration
CORS_ORIGINS=http://localhost,https://tu-dominio.com

# File Upload
MAX_FILE_SIZE=10MB
MAX_REQUEST_SIZE=10MB

# SSL/Security (production only)
HTTPS_ENABLED=false
SSL_CERT_PATH=/path/to/cert.crt
SSL_KEY_PATH=/path/to/private.key
