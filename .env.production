# Environment variables for production deployment  
DOCKER_REGISTRY=docker.io
DOCKER_REPO=gymetra
IMAGE_TAG=latest

# Database Configuration - CAMBIAR EN PRODUCCIÓN
POSTGRES_PASSWORD=PRODUCTION_DB_PASSWORD_CHANGE_ME
POSTGRES_DB=gymdb_production

# Application Configuration
SPRING_PROFILES_ACTIVE=production
LOG_LEVEL=WARN

# Email Configuration (production)
MAIL_HOST=smtp.gmail.com
MAIL_PORT=587
MAIL_USERNAME=production@gymetra.com
MAIL_PASSWORD=PRODUCTION_MAIL_PASSWORD_CHANGE_ME

# JWT Configuration - CAMBIAR EN PRODUCCIÓN
JWT_SECRET=PRODUCTION_JWT_SECRET_VERY_SECURE_CHANGE_ME_2024
JWT_EXPIRATION=36000000

# Cors Configuration
CORS_ORIGINS=https://gymetra.com,https://www.gymetra.com

# File Upload
MAX_FILE_SIZE=10MB
MAX_REQUEST_SIZE=10MB

# SSL/Security
HTTPS_ENABLED=true
SSL_CERT_PATH=/opt/ssl/gymetra.crt
SSL_KEY_PATH=/opt/ssl/gymetra.key
